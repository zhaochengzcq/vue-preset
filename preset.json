{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-service": {},
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-pwa": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save"]
    }
  },
  "configs": {
    "vue": {
      "css": {
        "modules": false,
        "sourceMap": false,
        "loaderOptions": {}
      },
      "productionSourceMap": false,
      "transpileDependencies": [],
      "configureWebpack": {},
      "devServer": {
        "host": "localhost",
        "port": 8080,
        "hot": true,
        "hotOnly": false,
        "open": true,
        "https": false,
        "overlay": {
          "warnings": false,
          "errors": true
        }
      }
    }
  },
  "router": true,
  "routerHistoryMode": false,
  "vuex": true,
  "cssPreprocessor": "stylus"
}
